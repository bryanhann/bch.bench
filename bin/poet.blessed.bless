#!/bin/bash
cat <<HEREDOC > $2
#!/bin/bash
# THIS SCRIPT WAS AUTOGENERATED BY THE PROGRAM
#    [poet.blessed.baptize]
# to serve as a blessed proxy for the file
#    [$1].
# Except for the first line exporting this path
# to the environment variable M, all such scripts
# are identical.

export M="$1"

